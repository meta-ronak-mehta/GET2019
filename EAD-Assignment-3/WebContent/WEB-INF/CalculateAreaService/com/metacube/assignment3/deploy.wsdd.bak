<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CalculateAreaService WSDL service -->

  <service name="CalculateArea" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://assignment3.metacube.com"/>
      <parameter name="wsdlServiceElement" value="CalculateAreaService"/>
      <parameter name="schemaQualified" value="http://assignment3.metacube.com"/>
      <parameter name="wsdlServicePort" value="CalculateArea"/>
      <parameter name="className" value="com.metacube.assignment3.CalculateAreaSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CalculateArea"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="areaOfTriangle" qname="operNS:areaOfTriangle" xmlns:operNS="http://assignment3.metacube.com" returnQName="retNS:areaOfTriangleReturn" xmlns:retNS="http://assignment3.metacube.com" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:side1" xmlns:pns="http://assignment3.metacube.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:side2" xmlns:pns="http://assignment3.metacube.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:side3" xmlns:pns="http://assignment3.metacube.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="areaOfTriangle"/>

  </service>
</deployment>
